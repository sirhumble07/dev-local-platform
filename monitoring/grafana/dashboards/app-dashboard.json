{
  "id": null,
  "uid": "app-dashboard",
  "title": "App - HTTP Metrics",
  "schemaVersion": 27,
  "version": 1,
  "panels": [
    {
      "type": "graph",
      "title": "HTTP Request Rate",
      "targets": [
        {
          "expr": "sum by (endpoint) (rate(http_requests_total[1m]))",
          "format": "time_series",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 0 }
    },
    {
      "type": "graph",
      "title": "HTTP Request Latency (histogram)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[1m])) by (le))",
          "format": "time_series",
          "refId": "A"
        }
      ],
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 8 }
    }
  ],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" }
}
